{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/ClusterAutoscalingValuesList",
    "definitions": {
        "ClusterAutoscalingValues": {
            "required": [
                "name"
            ],
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Name is the name of the associated Karpenter NodePool object"
                },
                "labels": {
                    "items": {
                        "$ref": "#/definitions/DomainLabels"
                    },
                    "additionalProperties": false,
                    "type": "array",
                    "description": "Labels are the domain labels that should be present on the NodePool"
                },
                "spec": {
                    "$ref": "#/definitions/NodePoolSpec",
                    "additionalProperties": false,
                    "description": "Spec defines the NodePool fields"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Cluster Autoscaling Values",
            "description": "ClusterAutoscalingValues represents the fields in a cluster autoscaling recommendation."
        },
        "ClusterAutoscalingValuesList": {
            "properties": {
                "values": {
                    "items": {
                        "$ref": "#/definitions/ClusterAutoscalingValues"
                    },
                    "additionalProperties": false,
                    "type": "array"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Cluster Autoscaling Values List",
            "description": "ClusterAutoscalingValuesList represents a list of cluster autoscaling recommendations. These recommendations are formatted into Karpenter NodePools to be applied in the customer's environment."
        },
        "DomainLabels": {
            "required": [
                "name",
                "value"
            ],
            "properties": {
                "name": {
                    "type": "string"
                },
                "value": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Domain Labels",
            "description": "DomainLabels represents the set of domain labels that should be present on the NodePool."
        },
        "NodeClassRef": {
            "required": [
                "group",
                "kind",
                "name"
            ],
            "properties": {
                "group": {
                    "type": "string",
                    "description": "Group represents the API group of the NodeClass"
                },
                "kind": {
                    "type": "string",
                    "description": "Kind represents the NodeClass resource kind"
                },
                "name": {
                    "type": "string",
                    "description": "Name is the name of the NodeClass"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Node Class Ref",
            "description": "NodeClassRef holds a reference to a cloud provider's NodeClass."
        },
        "NodePoolSpec": {
            "properties": {
                "node_class_ref": {
                    "$ref": "#/definitions/NodeClassRef",
                    "additionalProperties": false
                },
                "taints": {
                    "items": {
                        "$ref": "#/definitions/Taints"
                    },
                    "additionalProperties": false,
                    "type": "array"
                },
                "requirements": {
                    "items": {
                        "$ref": "#/definitions/Requirements"
                    },
                    "additionalProperties": false,
                    "type": "array"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Node Pool Spec",
            "description": "NodePoolSpec represents the Spec of the NodePool that matches the recommendation produced."
        },
        "Requirements": {
            "required": [
                "key",
                "operator",
                "values"
            ],
            "properties": {
                "key": {
                    "type": "string"
                },
                "operator": {
                    "type": "string"
                },
                "values": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Requirements",
            "description": "Requirements specifies a Karpenter NodePool requirement to specify constraints of the NodePool"
        },
        "Taints": {
            "required": [
                "key",
                "value",
                "effect"
            ],
            "properties": {
                "key": {
                    "type": "string"
                },
                "value": {
                    "type": "string"
                },
                "effect": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Taints",
            "description": "Taints are the minimum set of taints that should be set on the NodePool"
        }
    }
}